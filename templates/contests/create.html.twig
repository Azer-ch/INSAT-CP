{% extends 'headerBase.html.twig' %}
{% block body %}

    <div class="container">
        <form action="{{ path('create_contest') }}" method="post">
            <div class="form-group">
                <label for="">Title</label>
                <input type="text" class="form-control" id="" name="title" aria-describedby="" placeholder="Enter Title">
            </div>
            <div class="form-group">
                <label for="">start time</label>
                <input type="date" class="form-control" name="date" id="" placeholder="enter start time">
            </div>
            <div class="form-group">
                <label for="">duration</label>
                <input type="number" class="form-control" name="duration" id="" placeholder="enter contest duration">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
{% endblock %}